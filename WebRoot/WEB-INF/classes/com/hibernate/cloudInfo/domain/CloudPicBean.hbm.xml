<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.cloudInfo.domain.CloudPicBean" table="cloudPic"
		>
		<id name="id" type="java.lang.String" column="ID">
			<generator class="uuid.hex" />
		</id>
		<property name="aoJianDataId" type="java.lang.String">
			<column name="aoJianDataId" />
		</property>
		<property name="yuTuType" type="java.lang.Integer">
			<column name="yuTuType" />
		</property>
		<property name="section" type="java.lang.String">
			<column name="section" />
		</property>
		<property name="ViewOrder" type="java.lang.Integer">
			<column name="ViewOrder" />
		</property>
		<property name="picUrl" type="java.lang.String">
			<column name="picUrl" />
		</property>
		<property name="creatTime" type="java.util.Date">
			<column name="creatTime" />
		</property>

	</class>
</hibernate-mapping>

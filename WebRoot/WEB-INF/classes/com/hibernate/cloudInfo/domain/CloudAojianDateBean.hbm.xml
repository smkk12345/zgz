<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.cloudInfo.domain.CloudAojianDateBean" table="cloudAojianDate"
		>
		<id name="id" type="java.lang.String" column="ID">
			<generator class="uuid.hex" />
		</id>
		<property name="zyAoJianGrainNum" type="java.lang.String">
			<column name="zyAoJianGrainNum"/>
		</property>
		<property name="aoJianDataId" type="java.lang.String">
			<column name="aoJianDataId"/>
		</property>
		<property name="lqTime" type="java.util.Date">
			<column name="lqTime"/>
		</property>
		<property name="creatTime" type="java.util.Date">
			<column name="creatTime"/>
		</property>
	</class>
</hibernate-mapping>

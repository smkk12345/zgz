<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="userServiceImpl" class="com.hibernate.userInfo.service.impl.UserInfoServiceImpl">
		<property name="dao" ref="userDao"></property>
	</bean>
	<bean id="userDao" class="com.hibernate.userInfo.dao.impl.UserInfoDaoImpl"></bean>

	<bean id="roleServiceImpl" class="com.hibernate.userInfo.service.impl.RoleServiceImpl">
		<property name="dao" ref="roleDao"></property>
	</bean>
	<bean id="roleDao" class="com.hibernate.userInfo.dao.impl.RoleDaoImpl"></bean>
	
	<bean id = "indexNumService" class="com.hibernate.houseinfo.service.IndexNumService">
		<property name="indexNumDao" ref="indexNumDao"></property>
	</bean>
	<bean id="indexNumDao" class="com.hibernate.houseinfo.dao.IndexNumDao"></bean>
	
	<bean id="houseBasicService" class="com.hibernate.houseinfo.service.HouseBasicService">
		<property name="houseBasicDao" ref="houseBasicDao"></property>
		<property name="otherInfoDao" ref="otherInfoDao"></property>
		<property name="vacatePeopleDao" ref="vacatePeopleDao"></property>
		
	</bean>
	<bean id = "houseBasicDao" class="com.hibernate.houseinfo.dao.HouseBasicDao"></bean>
	<bean id = "otherInfoDao" class="com.hibernate.houseinfo.dao.OtherInfoDao"></bean>
	<bean id = "vacatePeopleDao" class="com.hibernate.houseinfo.dao.VacatePeopleDao"></bean>
</beans>